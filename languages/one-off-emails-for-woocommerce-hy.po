msgid "Heading: "
msgstr "Վերնագիր. "

msgid "Heading and Message fields are required to preview the email."
msgstr "Վերնագիր և հաղորդագրություն դաշտերը պարտադիր են նախադիտման համար."

msgid "Message: "
msgstr "Հաղորդագրություն. "

msgid "No valid email addresses provided."
msgstr "Վավեր էլեկտրոնային հասցեներ չեն տրամադրվել."

msgid "One-Off Emails"
msgstr "Միանգամյա նամակներ"

msgid "Preview"
msgstr "Նախադիտման"

msgid "Reply To Email: "
msgstr "Պատասխանի էլ. հասցե. "

msgid "Reply To Name: "
msgstr "Պատասխանի անուն. "

msgid "Send"
msgstr "Ուղարկել"

msgid "Subject: "
msgstr "Նյութ. "

msgid "The following fields are required to send an email: To, Subject, Heading, Message."
msgstr "Հետևյալ դաշտերը պարտադիր են նամակ ուղարկելու համար. ՈւմՍ ՆյութՍ ՎերնագիրՍ Հաղորդագրություն."

msgid "To: "
msgstr "Ում. "

msgid "WooCommerce One-Off Emails"
msgstr "WooCommerce միանգամյա նամակներ"
